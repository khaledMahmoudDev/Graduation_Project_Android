<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".calender.CalendarView">

    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="start at :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show_start_time"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show_end_time"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            android:layout_marginStart="20dp"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="End At : "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            app:layout_constraintStart_toEndOf="@+id/show_start_time"
            android:layout_marginStart="40dp"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="Location Is :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:layout_marginTop="100dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/show_Location"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show_Location"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBaseline_toBaselineOf="@+id/textView10"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="Title "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show_title"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="89dp" app:layout_constraintTop_toBottomOf="@+id/show_Location"/>
    <TextView
            android:textColor="#000000"
            android:textSize="20sp"
            android:text="Details "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show_details"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="86dp"
            app:layout_constraintTop_toBottomOf="@+id/show_title"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16"
            app:layout_constraintTop_toTopOf="@+id/show_details" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="64dp"/>
</android.support.constraint.ConstraintLayout>